style.
  body {
    padding: 0;
    margin: 0;
    background-color: #f2f2f2;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    font-family: 'Helvetica Neue';
    font-weight: 200;
  }
  h1 {
    font-size: 5.1em;
    color: #007AFF;
    font-family: 'Helvetica Neue';
    font-weight: 100;
    cursor: default;
    display: block;
  }
  input {
    font-family: 'Helvetica Neue';
    font-weight: 200;
    width: 70%;
    height: 40px;
    border: none;
    outline: none;
    color: #000;
    background-color: transparent;
    font-size: 20px;
    line-height: 40px;
  }
  input:focus {
    color: #007AFF;
  }
  a {
    margin: 0;
    outline: none;
    font-family: 'Helvetica Neue';
    font-weight: 200;
    color: #007AFF;
    text-decoration: none;
    line-height: 20px;
    border:none;
    box-shadow: inset 0 0 1px rgba(0, 0, 0, 1);
    padding: 5px 5px 5px 5px;
    border-radius: 3px;
    width: 40%;
    display: inline-block;
    cursor: pointer;
  }
  .question {
    color: #007AFF;
    position: absolute;
    right: 5px;
    top: 5px;
  }
  .what {
    position: absolute;
    right: 8px;
    display: block;
    font-size: 10px;
    color: #333;
    margin-right: 10px;
    width: 65px;
    line-height: 23px;
  }
  .ico {
    cursor: pointer;
    display: inline-block;
    margin: 0;
    padding: 0;
    outline: none;
    text-decoration: none;
    border:none;
    box-shadow: none;
  }
  ul {
    font-size: 12px;
    max-width: 400px;
    width: auto;
    margin: 0px auto;
  }
  b a, p a {
    line-height: auto;
    border:none;
    box-shadow: none;
    padding: none;
    border-radius: 0px;
  }
  .gh a {
    line-height: auto;
    border:none;
    box-shadow: none;
    padding: none;
    border-radius: 0px;
  }